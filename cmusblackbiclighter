 common   mouse_scroll_up win-up
 common   n              search-next
 common   o              toggle play_sorted
 common   p              win-mv-after
 common   page_down      win-page-down
 common   page_up        win-page-up
 common   q              quit -i
 common   r              toggle repeat
 common   right          seek +5
 common   s              toggle shuffle
 common   space          win-toggle
 common   t              toggle show_remaining_time
